{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/uploads/$1"
        },
        {
            "src": "/(.*\\.(html|css|js|png|jpg|jpeg|svg|ico))$",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}